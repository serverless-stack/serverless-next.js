  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.1",
  "description": "Provides handlers that can be used in CloudFront Lambda@Edge to deploy next.js applications to the edge",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "build-lambda-at-edge": "dist/command.js"
  },
  "scripts": {
